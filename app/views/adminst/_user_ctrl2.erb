<div class="panel panel-default col-md-12" style="padding:0;">
    
    
    <!-- Default panel contents -->
    <div class="panel-heading" style="font-size:25px;">
        회원가입자 <b>총 <%= User.count %></b>
    </div>
    <div class="panel-body">
        <p>총 DGUlion 페이지 회원가입자 현황 thead는 User.attribute_names</p>
    </div>
    
                        <!--<div class="panel-body">-->
                        <!--    <div class="dataTable_wrapper">    -->
    <div class="dataTable_wrapper">
    <!-- Table -->
    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
    <thead>
        <tr style="font-weight:bold;">
            <th>id</th>
            <th>권한</th>
            <th>닉네임</th>
            <th>이름</th>
            <th>전화번호</th>
            <th>이메일</th>
            <th>가입일</th>
            <th>로그인횟수</th>
            <th>최종방문일</th>
            <th>부가기능</th>
        </tr>
    </thead>
    <tbody>
        <% User.all.each do |user| %>
            <%
                if user.mb_open == 10
                    mb_open = '최고관리자'
                elsif user.mb_open == 9
                    mb_open = '사이트 관리자'
                elsif user.mb_open == 8
                    mb_open = '고위임직원'
                elsif user.mb_open == 7
                    mb_open = '일반임직원'
                elsif user.mb_open == 6
                    mb_open = '일반_특별관리자'
                elsif user.mb_open == 5
                    mb_open = '일반_보통관리자'
                elsif user.mb_open == 4
                    mb_open = '정회원'
                elsif user.mb_open == 3
                    mb_open = '후원회원'
                elsif user.mb_open == 2
                    mb_open = '인터넷회원'
                elsif user.mb_open == 1
                    mb_open = '비회원'
                elsif user.mb_open == 0
                    mb_open = '방문자'
                elsif user.mb_open == nil
                    mb_open = '방문자'
                end
            %>
            <tr>
                <td><%= user.id %></td>
                <th><%= mb_open %></th>
                <td><%= user.nickname %></td>
                <td><%= user.username %></td>
                <td><%= user.phonenumber %></td>
                <td><%= user.email %></td>
                <td><%= user.created_at %></td>
                <td><%= user.sign_in_count %></td>
                <td><%= user.updated_at %></td>
                <td><a id="mod_user_<%= user.id %>" class="btn btn-blue">수정</a> <a href="<%= user.id %>" class="btn btn-blue">삭제</a></td>
            </tr>
        <% end %>
    </tbody>
  </table>
  </div>
</div>

    <!-- jQuery -->
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="/bower_components/metisMenu/dist/metisMenu.min.js"></script>

    <!-- DataTables JavaScript -->
    <script src="/bower_components/datatables/media/js/jquery.dataTables.min.js"></script>
    <script src="/bower_components/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="/dist/js/sb-admin-2.js"></script>

    <!-- Page-Level Demo Scripts - Tables - Use for reference -->
    <script>
    $(document).ready(function() {
        $('#dataTables-example').DataTable({
                responsive: true
        });
    });
    </script>